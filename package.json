{
  "name": "hdo-blog",
  "version": "1.0.0",
  "description": "Holder de ord blog",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "12"
  },
  "repository": "https://github.com/holderdeord/hdo-blog",
  "license": "MIT",
  "author": "Nikolai Kristiansen <nikolaik@gmail.com>, Jari Bakken <jari@holderdeord.no>",
  "scripts": {
